   <rect>
    <x>0</x>
    <y>0</y>
    <width>285</width>
    <height>277</height>
   </rect>
  </property>
  <property name="sizePolicy">
   <sizepolicy hsizetype="Minimum" vsizetype="Minimum">
    <horstretch>0</horstretch>
    <verstretch>0</verstretch>
   </sizepolicy>
  </property>
  <property name="minimumSize">
   <size>
    <width>285</width>
    <height>277</height>
   </size>
  </property>
  <property name="sizeGripEnabled" stdset="0">
   <bool>true</bool>
  </property>
  <layout class="QGridLayout" name="gridLayout_3">
   <item row="0" column="0">
    <widget class="KTabWidget" name="m_tabWidget">
     <property name="currentIndex">
      <number>0</number>
     </property>
     <widget class="QWidget" name="tabSequencer">
      <attribute name="title">
       <string>Sequencer</string>
      </attribute>
      <layout class="QGridLayout" name="gridLayout">
       <item row="0" column="0">
        <widget class="QLabel" name="label">
         <property name="font">
          <font>
           <weight>75</weight>
           <bold>true</bold>
          </font>
         </property>
         <property name="text">
          <string>Queue Default Settings</string>
         </property>
        </widget>
       </item>
       <item row="1" column="0">
        <widget class="QLabel" name="lblTempo">
         <property name="text">
          <string>Tempo (BPM)</string>
         </property>
         <property name="wordWrap">
          <bool>false</bool>
         </property>
         <property name="buddy">
          <cstring>m_tempo</cstring>
         </property>
        </widget>
       </item>
       <item row="1" column="1">
        <widget class="KIntSpinBox" name="m_tempo">
         <property name="whatsThis">
          <string>The number of beats per minute, or music speed. This is a default value; MIDI files may change this value.</string>
         </property>
         <property name="minimum">
          <number>24</number>
         </property>
         <property name="maximum">
          <number>480</number>
         </property>
         <property name="maxValue" stdset="0">
          <number>480</number>
         </property>
         <property name="minValue" stdset="0">
          <number>24</number>
         </property>
        </widget>
       </item>
       <item row="2" column="0">
        <widget class="QLabel" name="lblResolution">
         <property name="text">
          <string>Resolution</string>
         </property>
         <property name="wordWrap">
          <bool>false</bool>
         </property>
         <property name="buddy">
          <cstring>m_resolution</cstring>
         </property>
        </widget>
       </item>
       <item row="2" column="1">
        <widget class="KIntSpinBox" name="m_resolution">
         <property name="whatsThis">
          <string>The number of MIDI ticks per quarter note. This is a default value; a MIDI file (SMF) may change this value.</string>
         </property>
         <property name="minimum">
          <number>48</number>
         </property>
         <property name="maximum">
          <number>960</number>
         </property>
         <property name="maxValue" stdset="0">
          <number>960</number>
         </property>
         <property name="minValue" stdset="0">
          <number>48</number>
         </property>
        </widget>
       </item>
